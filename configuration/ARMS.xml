                <field>island</field>
                <field>islandGroup</field>
                <field>region/Location</field>
                <field>locality</field>
                <field>depthOfBottomMeters</field>
                <field>stateProvince</field>
                <field>actualDeploymentTimeOfDay</field>
                <field>numPlates</field>
                <field>armsModel</field>
                <field>laminates</field>
                <field>new/reused</field>
                <field>hasScrubbieLayer</field>
                <field>intentToBarcode</field>
                <field>intentToMetabarcode</field>
                <field>intentToCollectOtherDatatypes</field>
                <field>intentToPhotographPlates</field>
                <field>intentToPhotographSpecimens</field>
                <field>intendedRecoveryDate</field>
                <field>intendedDeploymentDate</field>
                <field>intendedSoakTimeInMonths</field>
                <field>continentOcean</field>
                <field>country</field>
                <field>weightsAttached</field>
                <field>actualDeploymentDate</field>
                <field>actualRecoveryDate</field>
            </rule>
            <rule type="datasetContainsExtraColumns" level="warning"/>
            <rule type='uniqueValue' column='deploymentID' level='error'/>
            <rule type='controlledVocabulary' column='country' list='country' level='error'/>
            <rule type="latLngChecker" decimalLatitude="decimalLatitude" decimalLongitude="decimalLongitude"
                  level="error"/>
            <rule type='validateNumeric' column='depthInMeters' level='warning' value='%3E%3D0'/>
            <rule type='validateNumeric' column='depthOfBottomMeters' level='warning' value='%3E%3D0'/>
            <rule type='validateNumeric' column='errorRadius' level='warning' value='%3E%3D0'/>
            <rule type='checkInXMLFields' column='hasScrubbieLayer' list="yesNo" level="error"/>
            <rule type='checkInXMLFields' column='laminates' list="yesNo" level="error"/>
            <rule type='checkInXMLFields' column='weightsAttached' list="yesNo" level="error"/>
            <rule type='checkInXMLFields' column='new/reused' list="newReused" level="error"/>
            <rule type='checkInXMLFields' column='continentOcean' list="continentOcean" level="error"/>
            <rule type='checkInXMLFields' column='intentToBarcode' list="intent" level="error"/>
            <rule type='checkInXMLFields' column='intentToCollectOtherDatatypes' list="intent" level="error"/>
            <rule type='checkInXMLFields' column='intentToMetabarcode' list="intent" level="error"/>
            <rule type='checkInXMLFields' column='intentToPhotographPlates' list="intent" level="error"/>
            <rule type='checkInXMLFields' column='intentToPhotographSpecimens' list="intent" level="error"/>
            <rule type='checkInXMLFields' column='horizontalDatum' list="horizontalDatum" level="warning"/>
            <rule type='checkInXMLFields' column='substrateType' list="substrateType" level="warning"/>
            <rule type="isInteger" column="numPlates" level="error"/>
            <rule type="isInteger" column="numReplicatesInSet" level="error"/>
            <rule type="isInteger" column="intendedSoakTimeInMonths" level="error"/>
            <rule type="isInteger" column="depthInMeters" level="error"/>
            <rule type="isInteger" column="depthOfBottomMeters" level="error"/>
            <rule type="isInteger" column="durationToProcessing" level="error"/>
            <rule type="isValidUrl" column="photoURL" level="warning"/>
        </worksheet>
